<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="jelly:jetty" elementFormDefault="qualified">
  <xsd:annotation>
    <xsd:documentation>
      <p>Tags for starting Jetty HTTP servers and procesing HTTP requests using Jelly scripts</p>
    </xsd:documentation>
  </xsd:annotation>
  <xsd:element name="jettyHttpServer">
    <xsd:annotation>
      <xsd:documentation>Declare an instance of a Jetty http server
        <authortag>rtl</authortag>
        <versiontag>$Id: JettyHttpServerTag.java 480470 2006-11-29 08:22:46Z bayard $</versiontag>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="var">
        <xsd:annotation>
          <xsd:documentation>Setter for property var.
          <paramtag>var New value of property var.</paramtag>
        </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="logFileName">
        <xsd:annotation>
          <xsd:documentation>Setter for property logFileName.
          <paramtag>logFileName New value of property logFileName.</paramtag>
        </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="trim">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="tagLib">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="escapeText">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="postRequest">
    <xsd:annotation>
      <xsd:documentation>Respond to a POST request to a Jetty http server
        <authortag>rtl</authortag>
        <versiontag>$Id: PostRequestTag.java 480470 2006-11-29 08:22:46Z bayard $</versiontag>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="trim">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="tagLib">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="escapeText">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="realm">
    <xsd:annotation>
      <xsd:documentation>Declare a user realm for a Jetty http server
        <authortag>rtl</authortag>
        <versiontag>$Id: RealmTag.java 480470 2006-11-29 08:22:46Z bayard $</versiontag>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="name">
        <xsd:annotation>
          <xsd:documentation>Setter for property name.
          <paramtag>name New value of property name.</paramtag>
        </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="config">
        <xsd:annotation>
          <xsd:documentation>Setter for property config.
          <paramtag>config New value of property config.</paramtag>
        </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="trim">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="tagLib">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="escapeText">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="deleteRequest">
    <xsd:annotation>
      <xsd:documentation>Respond to a DELETE request to a Jetty http server
        <authortag>rtl</authortag>
        <versiontag>$Id: DeleteRequestTag.java 480470 2006-11-29 08:22:46Z bayard $</versiontag>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="trim">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="tagLib">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="escapeText">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="notFoundHandler">
    <xsd:annotation>
      <xsd:documentation>Declare a not found resource handler for a Jetty http server
        <authortag>rtl</authortag>
        <versiontag>$Id: NotFoundHandlerTag.java 480470 2006-11-29 08:22:46Z bayard $</versiontag>
      </xsd:documentation>
    </xsd:annotation>
